@model RssFeeder.Resources.RssLinkResource

    <div class="card rss-link-card">
        <div class="card-header">
            <img src="@Model.ImageUrl" alt="RSS Link Image" />
        </div>
        <div class="card-body">
            <h5 class="card-title">@Model.Name</h5>
            <p class="card-text">@Model.Description</p>
        </div>
        <div class="card-footer">
            <button class="rss-feed-btn rss-feed-card-btn btn btn-outline-danger">Delete</button>
            <button type="submit" class="rss-feed-btn rss-feed-card-btn btn btn-outline-primary">Register</button>
        </div>
    </div>
